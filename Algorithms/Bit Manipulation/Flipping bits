#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   unsigned  int t;
    scanf("%d",&t);
    for(int i=0;i<t;i++)
    {
       long unsigned int n,k;
        scanf("%d",&n);
        k=pow(2,32);
        k=k-1;
        k=n^k;
        printf("%ld\n",k);
    }
    return 0;
}

